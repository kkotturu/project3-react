{
  "name": "brewnbarks",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "heroku-postbuild": "cd client && npm install && NODE_ENV=production"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "headers": {
    "Content-Type": "application/json",
    "Access-Control-Allow-Origin": "*"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "mysql2": "^1.6.5",
    "react-simple-card": "^1.0.11",
    "sequelize": "^5.8.7"
  }
}
